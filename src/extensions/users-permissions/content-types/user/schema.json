{
  "kind": "collectionType",
  "collectionName": "up_users",
  "info": {
    "name": "user",
    "description": "",
    "singularName": "user",
    "pluralName": "users",
    "displayName": "User"
  },
  "options": {
    "draftAndPublish": false
  },
  "attributes": {
    "username": {
      "type": "string",
      "minLength": 3,
      "unique": true,
      "configurable": false,
      "required": true
    },
    "email": {
      "type": "email",
      "minLength": 6,
      "configurable": false,
      "required": true
    },
    "provider": {
      "type": "string",
      "configurable": false
    },
    "password": {
      "type": "password",
      "minLength": 6,
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "resetPasswordToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmationToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmed": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "blocked": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "role": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.role",
      "inversedBy": "users",
      "configurable": false
    },
    "userId": {
      "type": "uid"
    },
    "firstName": {
      "type": "string"
    },
    "lastName": {
      "type": "string"
    },
    "countryDialCode": {
      "type": "string"
    },
    "contactNumber": {
      "type": "string"
    },
    "userRole": {
      "type": "enumeration",
      "enum": [
        "admin",
        "customer"
      ]
    },
    "admin": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::admin.admin",
      "inversedBy": "users_permissions_user"
    },
    "super_admin": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::super-admin.super-admin",
      "inversedBy": "users_permissions_user"
    },
    "customer": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::customer.customer",
      "mappedBy": "users_permissions_user"
    },
    "bookings": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::booking.booking",
      "mappedBy": "users_permissions_user"
    },
    "memberbookings": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::booking.booking",
      "inversedBy": "members"
    },
    "credit_transaction": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::credit-transaction.credit-transaction",
      "mappedBy": "users_permissions_user"
    },
    "guests": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::guest.guest",
      "mappedBy": "users_permissions_users"
    }
  }
}
